
modules = ["python-3.12"]

[nix]
channel = "stable-25_05"
packages = ["iana-etc"]

[workflows]
[workflows.backend]
language = "python3"
start = ["cd", "backend", "&&", "python3", "app.py"]

[deployment]
run = ["python3", "backend/app.py"]
deploymentTarget = "cloudrun"

[[ports]]
localPort = 5000
externalPort = 80
